                <div class="btn-group">
                  <button
                    class="btn-icon"
                    aria-label="previous"
                    data-slider-prev
                  >
                    <ion-icon name="arrow-back" aria-hidden="true"></ion-icon>
                  </button>

                  <button class="btn-icon" aria-label="next" data-slider-next>
                    <ion-icon
                      name="arrow-forward"
                      aria-hidden="true"
                    ></ion-icon>
                  </button>
                </div>
              </div>

              <div class="slider" data-slider>
                <ul class="slider-list" data-slider-container>
                  <% arrTop.forEach(itemTop => { %>
                  <li class="slider-item">
                    <a href="  <%- itemTop.social %> " class="slider-card">
                      <figure
                        class="slider-banner img-holder"
                        style="--width: 507; --height: 618"
                      >
                        <img
                          src="<%- itemTop.image || '/images/404.png' %>"
                          width="507"
                          height="618"
                          loading="lazy"
                          alt="<%- itemTop.name %>"
                          class="img-cover"
                        />
                      </figure>

                      <div class="slider-content">
                        <span class="slider-title"> <%- itemTop.name %> </span>

                        <p class="slider-subtitle">
                          <%- itemTop.rank %> <br />
                          <%- itemTop.competition %> <br />
                          <%- itemTop.date %>
                        </p>
                      </div>
                    </a>
                  </li>

                  <% }); %>
                </ul>
              </div>
            </div>
