                <!-- Lorem ipsum dolor sit amet, consectetur adipisicing elit. Cum
                temporibus voluptatum sint. Vero et, nihil facilis veniam
                necessitatibus repellendus dolorem officia maiores quasi. -->
              </p>

              <div class="input-wrapper">
                <input
                  type="email"
                  name="email_address"
                  placeholder="Type your email address"
                  required
                  class="input-field"
                  autocomplete="off"
                />

                <button class="btn btn-primary">
                  <a href="#"><span class="span">Contact</span></a>
                  <ion-icon
                    name="arrow-forward-outline"
                    aria-hidden="true"
                  ></ion-icon>
                </button>
              </div>
            </div>

            <div class="church-banner">
              <img
                src="/images/pattern-2.svg"
                width="27"
                height="26"
                alt="shape"
                class="shape shape-1"
              />

              <img
                src="/images/pattern-3.svg"
                width="27"
                height="26"
                alt="shape"
                class="shape shape-2"
              />
            </div>

            <img
